git remote-v
git remote set-url origin https://github.com/meliason13/ReproRehab_Pod3_ME.git
git remote-v

git config --global credential.helper osxkeychain
git config --global --get credential.helper

install.packages(c("usethis","gitcreds"))   # safe to re-run
usethis::create_github_token()


gitcreds::gitcreds_set(url = "https://github.com")
# paste your token at the prompt, press Enter
